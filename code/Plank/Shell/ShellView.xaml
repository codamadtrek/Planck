<Window x:Class="LazyE9.Plank.Shell.ShellView"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
				xmlns:ui="clr-namespace:LazyE9.Plank.Shell"
        mc:Ignorable="d" 
				d:DataContext="{d:DesignInstance ui:ShellViewModel, IsDesignTimeCreatable=True}"
        Title="Plank" Height="720" Width="850">
    <Grid Background="{StaticResource Brush_ApplicationBackground}">
        <ContentControl Content="{Binding NavigationManager.CurrentView}" Margin="20"/>
    </Grid>
</Window>
