<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <Color x:Key="FocusedColor">#29ABE2</Color>

    <SolidColorBrush x:Key="Brush_LabelColor" Color="#808080"/>
    <SolidColorBrush x:Key="Brush_PrimaryColor" Color="#175B97"/>
    <SolidColorBrush x:Key="Brush_DisabledColor" Color="#B3B3B3"/>
    <SolidColorBrush x:Key="Brush_FocusedColor" Color="{StaticResource FocusedColor}"/>
    <SolidColorBrush x:Key="Brush_ControlBackground" Color="White"/>
    <SolidColorBrush x:Key="Brush_ControlBorder" Color="#FFB3B3B3" />
    <SolidColorBrush x:Key="Brush_ControlForeground" Color="#333333"/>
    <SolidColorBrush x:Key="Brush_ControlReadOnlyBackground" Color="#E6E6E6"/>
    <SolidColorBrush x:Key="Brush_MainActionBackground" Color="#F15A24"/>
    <SolidColorBrush x:Key="Brush_ApplicationBackground" Color="#FFEDEDED"/>


</ResourceDictionary>